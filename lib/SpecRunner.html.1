<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
  "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
  <title>Jasmine Spec Runner</title>
  <link rel="shortcut icon" type="image/png" href="lib/jasmine-1.1.0/jasmine_favicon.png">
  <link rel="stylesheet" type="text/css" href="lib/jasmine-1.1.0/jasmine.css">
  <script type="text/javascript" src="lib/jquery.js"></script>
  <script>
    $(document).ready(function() {
        var outerDiv = $('div.jasmine_reporter');
        var showPassed = $('#__jasmine_TrivialReporter_showPassed__');
        var showSkipped = $('#__jasmine_TrivialReporter_showSkipped__');
        
        showPassed.click(function() {
          outerDiv.toggleClass('show-passed');
        });
        
        showSkipped.click(function() {
          outerDiv.toggleClass('show-skipped');
        });
    });
  </script>
</head>

